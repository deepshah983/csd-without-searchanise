{"components/common/wallet-buttons":"{{#if wallet_buttons}}\r\n    {{{wallet_buttons}}}\r\n{{/if}}\r\n","components/products/add-to-cart":"<div id=\"add-to-cart-wrapper\" class=\"add-to-cart-wrapper\" {{#unless product.can_purchase}}style=\"display: none\"{{/unless}}>\r\n    {{#if product.stock_level '>' '1'}}\r\n    {{#if product.show_quantity_input}}\r\n        {{inject 'productQuantityErrorMessage'  (lang 'products.quantity_error_message')}}\r\n        <div class=\"form-field form-field--increments\">\r\n            <label class=\"form-label form-label--alternate\"\r\n                   for=\"qty[]\">{{lang 'products.quantity'}}</label>\r\n                <div class=\"form-increment\" data-quantity-change>\r\n                    <!-- <button class=\"button button--icon button-dec\" data-action=\"dec\">\t                    \r\n\t\t\t\t\t\t<span class=\"is-srOnly\">{{lang 'products.quantity_decrease'}}</span>\r\n\t                    <i class=\"icon\" aria-hidden=\"true\">\r\n\t                        <svg>\r\n\t                            <use xlink:href=\"#icon-keyboard-arrow-down\"/>\r\n\t                        </svg>\r\n\t                    </i>\r\n                \t</button> -->\r\n                    <select class=\"form-select qty-dropdown\" id=\"qty[]\">\r\n                        <option>1</option>\r\n                    </select>\r\n                    <!-- <input class=\"form-input form-input--incrementTotal\"\r\n                           id=\"qty[]\"\r\n                           name=\"qty[]\"\r\n                           type=\"tel\"\r\n                           value=\"{{#if product.min_purchase_quantity}}{{product.min_purchase_quantity}}{{else}}1{{/if}}\"\r\n                           data-quantity-min=\"{{product.min_purchase_quantity}}\"\r\n                           data-quantity-max=\"{{product.max_purchase_quantity}}\"\r\n                           min=\"1\"\r\n                           pattern=\"[0-9]*\"\r\n                           aria-live=\"polite\"> -->\r\n                    <!-- <button class=\"button button--icon button-inc\" data-action=\"inc\">\r\n                        <span class=\"is-srOnly\">{{lang 'products.quantity_increase' name=product.title}}</span>\r\n                        <i class=\"icon\" aria-hidden=\"true\">\r\n                            <svg>\r\n                                <use href=\"#icon-keyboard-arrow-up\"/>\r\n                            </svg>\r\n                        </i>\r\n                    </button> -->\r\n\t\t\t\t\t\r\n                </div>\r\n        </div>\r\n    {{/if}}\r\n\r\n    <div class=\"alertBox productAttributes-message\" style=\"display:none\">\r\n        <div class=\"alertBox-column alertBox-icon\">\r\n            <icon glyph=\"ic-success\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\r\n        </div>\r\n        <p class=\"alertBox-column alertBox-message\"></p>\r\n    </div>\r\n    {{/if}}\r\n    {{#or customer (unless settings.hide_price_from_guests)}}\r\n    <div class=\"add-to-cart-buttons {{#if product.pre_order}}pre-order-buttons{{/if}}\">\r\n            {{#if this.with_wallet_buttons}}\r\n                <div class=\"add-to-cart-wallet-buttons\" data-add-to-cart-wallet-buttons>\r\n                    {{> components/common/wallet-buttons}}\r\n                </div>\r\n            {{/if}}\r\n            <div class=\"form-action\">\r\n                \r\n                {{#comment}}\r\n                <a href=\"javascript:;\" data-product-name=\"{{product.title}}\" data-product-price=\"{{product.price.without_tax.value}}\" data-product-cat=\"{{product.category}}\" data-product-img=\"{{getImage product.main_image 'product_size' (cdn theme_settings.default_image_product)}}\" data-product-id=\"{{product.id}}\" data-wait-message=\"{{lang 'products.adding_to_cart'}}\" class=\"button button--primary {{#if product.options}}product-with-options{{else}}custom-add-to-cart{{/if}}\">\r\n                    {{#if product.pre_order}}{{lang 'products.pre_order'}}{{else}}{{lang 'products.add_to_cart'}}{{/if}}\r\n                </a>\r\n                {{/comment}}\r\n                \r\n                \r\n                <input id=\"form-action-addToCart\"\r\n                        data-product-name=\"{{product.title}}\" data-product-price=\"{{product.price.without_tax.value}}\" data-product-cat=\"{{product.category}}\" data-product-img=\"{{getImage product.main_image 'product_size' (cdn theme_settings.default_image_product)}}\" data-product-id=\"{{product.id}}\" data-wait-message=\"{{lang 'products.adding_to_cart'}}\" class=\"button button--primary {{#if product.options}}product-with-options{{else}}custom-add-to-cart{{/if}}\"\r\n                        data-wait-message=\"{{lang 'products.adding_to_cart'}}\"\r\n                        type=\"submit\"\r\n                        value=\"{{#if product.pre_order}}{{lang 'products.pre_order'}}{{else}}{{lang 'products.add_to_cart'}}{{/if}}\"\r\n                >\r\n               \r\n            </div>          \r\n        </div>\r\n    {{/or}}\r\n</div>\r\n"}
