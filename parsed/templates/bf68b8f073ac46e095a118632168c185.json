{"components/csd/breadcrumbs-product":"<ul class=\"breadcrumbs\" itemscope itemtype=\"https://schema.org/BreadcrumbList\">\r\n    {{#unless theme_settings.hide_breadcrumbs }}\r\n        {{#each breadcrumbs}}\r\n            {{#if @first}}\r\n                <li class=\"breadcrumb\" itemprop=\"itemListElement\" itemscope itemtype=\"https://schema.org/ListItem\">\r\n                    <a href=\"/\" class=\"breadcrumb-label\" itemprop=\"item\"><span itemprop=\"name\">Women Home</span></a>\r\n                </li>\r\n            {{else}}\r\n            {{/if}}\r\n        {{/each}}\r\n        {{#if product.brand}}\r\n        <li class=\"breadcrumb\" itemprop=\"itemListElement\" itemscope itemtype=\"https://schema.org/ListItem\">\r\n            <a href=\"{{product.brand.url}}\" class=\"breadcrumb-label\" itemprop=\"item\"><span itemprop=\"name\">{{product.brand.name}}</span></a>\r\n        </li>\r\n        {{/if}}\r\n        {{assignVar 'main-category' 'none'}}\r\n                    {{#each product.category}}\r\n                        {{#contains this 'Sale'}}\r\n                        {{else}}\r\n                            {{#contains this 'Featured Designers'}}\r\n                            {{else}}\r\n                                {{#contains this 'New'}}\r\n                                {{else}}\r\n                                    {{#contains this \"Clothing/\"}}\r\n                                        {{#if (getVar 'main-category') '===' 'none'}}\r\n                                            {{#each (split this '/')}}\r\n                                                {{#if @index '===' 0}}\r\n                                                    <li class=\"breadcrumb\" itemprop=\"itemListElement\" itemscope itemtype=\"https://schema.org/ListItem\"><a class=\"breadcrumb-label\" href=\"/women/{{dashcase (toLowerCase this)}}\" itemprop=\"item\"><span itemprop=\"name\"> {{this}}</span></a></li>\r\n                                                    {{assignVar 'main-category' 'clothing'}}\r\n                                                {{/if}}\r\n                                                {{#if @index '===' 1}}\r\n                                                    {{#contains this \"Jumpsuits\"}}\r\n                                                    <li class=\"breadcrumb\" itemprop=\"itemListElement\" itemscope itemtype=\"https://schema.org/ListItem\"><a class=\"breadcrumb-label\" href=\"/women/{{getVar 'main-category'}}/jumpsuits\" itemprop=\"item\"><span itemprop=\"name\"> {{this}}</span></a></li>\r\n                                                    {{else}}\r\n                                                    <li class=\"breadcrumb\" itemprop=\"itemListElement\" itemscope itemtype=\"https://schema.org/ListItem\"><a class=\"breadcrumb-label\" href=\"/women/{{getVar 'main-category'}}/{{dashcase (toLowerCase this)}}\" itemprop=\"item\"><span itemprop=\"name\"> {{this}}</span></a></li>\r\n                                                    {{/contains}}\r\n                                                {{/if}}\r\n                                            {{/each}}\r\n                                        {{/if}}\r\n                                    {{/contains}}\r\n                                    {{#contains this \"Bags/\"}}\r\n                                        {{#if (getVar 'main-category') '===' 'none'}}\r\n                                            {{#each (split this '/')}}\r\n                                                {{#if @index '===' 0}}\r\n                                                    <li class=\"breadcrumb\" itemprop=\"itemListElement\" itemscope itemtype=\"https://schema.org/ListItem\"><a class=\"breadcrumb-label\" href=\"/women/{{dashcase (toLowerCase this)}}\" itemprop=\"item\"><span itemprop=\"name\"> {{this}}</span></a></li>\r\n                                                    {{assignVar 'main-category' 'bags'}}\r\n                                                {{/if}}\r\n                                                {{#if @index '===' 1}}\r\n                                                    <li class=\"breadcrumb\" itemprop=\"itemListElement\" itemscope itemtype=\"https://schema.org/ListItem\"><a class=\"breadcrumb-label\" href=\"/women/{{getVar 'main-category'}}/{{dashcase (toLowerCase this)}}\" itemprop=\"item\"><span itemprop=\"name\"> {{this}}</span></a></li>\r\n                                                {{/if}}\r\n                                            {{/each}}\r\n                                        {{/if}}\r\n                                    {{/contains}}\r\n                                    {{#contains this \"Shoes/\"}}\r\n                                        {{#if (getVar 'main-category') '===' 'none'}}\r\n                                            {{#each (split this '/')}}\r\n                                                {{#if @index '===' 0}}\r\n                                                    <li class=\"breadcrumb\" itemprop=\"itemListElement\" itemscope itemtype=\"https://schema.org/ListItem\"><a class=\"breadcrumb-label\" href=\"/women/{{dashcase (toLowerCase this)}}\" itemprop=\"item\"><span itemprop=\"name\"> {{this}}</span></a></li>\r\n                                                    {{assignVar 'main-category' 'shoes'}}\r\n                                                {{/if}}\r\n                                                {{#if @index '===' 1}}\r\n                                                    <li class=\"breadcrumb\" itemprop=\"itemListElement\" itemscope itemtype=\"https://schema.org/ListItem\"><a class=\"breadcrumb-label\" href=\"/women/{{getVar 'main-category'}}/{{dashcase (toLowerCase this)}}\" itemprop=\"item\"><span itemprop=\"name\"> {{this}}</span></a></li>\r\n                                                {{/if}}\r\n                                            {{/each}}\r\n                                        {{/if}}\r\n                                    {{/contains}}\r\n                                    {{#contains this \"Accessories/\"}}\r\n                                        {{#if (getVar 'main-category') '===' 'none'}}\r\n                                            {{#each (split this '/')}}\r\n                                                {{#if @index '===' 0}}\r\n                                                    <li class=\"breadcrumb\" itemprop=\"itemListElement\" itemscope itemtype=\"https://schema.org/ListItem\"><a class=\"breadcrumb-label\" href=\"/women/{{dashcase (toLowerCase this)}}\" itemprop=\"item\"><span itemprop=\"name\"> {{this}}</span></a></li>\r\n                                                    {{assignVar 'main-category' 'accessories'}}\r\n                                                {{/if}}\r\n                                                {{#if @index '===' 1}}\r\n                                                    <li class=\"breadcrumb\" itemprop=\"itemListElement\" itemscope itemtype=\"https://schema.org/ListItem\"><a class=\"breadcrumb-label\" href=\"/women/{{getVar 'main-category'}}/{{dashcase (toLowerCase this)}}\" itemprop=\"item\"><span itemprop=\"name\"> {{this}}</span></a></li>\r\n                                                {{/if}}\r\n                                            {{/each}}\r\n                                        {{/if}}\r\n                                    {{/contains}}\r\n                                {{/contains}}\r\n                            {{/contains}}\r\n                        {{/contains}}\r\n                    {{/each}}\r\n       {{#comment}} \r\n        \r\n        {{#if product.brand}}\r\n            <li class=\"breadcrumb\" itemprop=\"itemListElement\" itemscope itemtype=\"https://schema.org/ListItem\">\r\n                <!-- <span>{{../product.brand.name}}: </span> -->\r\n                <span>{{product.brand.name}}: </span>\r\n            </li>\r\n            <li class=\"breadcrumb\" itemprop=\"itemListElement\" itemscope itemtype=\"https://schema.org/ListItem\">\r\n                <a href=\"{{product.brand.url}}\" class=\"breadcrumb-label\" itemprop=\"item\"><span itemprop=\"name\">All</span></a>\r\n                <meta itemprop=\"position\" content=\"1\" />\r\n            </li>\r\n            <li class=\"breadcrumb\" itemprop=\"itemListElement\" itemscope itemtype=\"https://schema.org/ListItem\">\r\n                <a href=\"{{product.brand.url}}\" class=\"breadcrumb-label\" itemprop=\"item\"><span itemprop=\"name\">Women</span></a>\r\n                <meta itemprop=\"position\" content=\"2\" />\r\n            </li>\r\n            {{#each breadcrumbs}}\r\n                {{#or @first @last}}\r\n                {{else}}\r\n                <li class=\"breadcrumb {{#if @last}}is-active{{/if}}\" itemprop=\"itemListElement\" itemscope itemtype=\"https://schema.org/ListItem\">\r\n                    {{#or @last (if url \"==\" null)}}\r\n                        <span class=\"breadcrumb-label \"><span itemprop=\"name\">{{name}}</span></span>\r\n                    {{else}}\r\n                        <a href=\"{{url}}\" class=\"breadcrumb-label \" itemprop=\"item\"><span itemprop=\"name\">{{name}}</span></a>\r\n                    {{/or}}\r\n                    <meta itemprop=\"position\" content=\"{{add @index 2}}\" />\r\n                </li>\r\n                {{/or}}\r\n            {{/each}}\r\n        {{else}}\r\n            {{#each breadcrumbs}}\r\n                {{#or @first @last}}\r\n                {{else}}\r\n                    <li class=\"breadcrumb {{#if @last}}is-active{{/if}}\" itemprop=\"itemListElement\" itemscope itemtype=\"https://schema.org/ListItem\">\r\n                        {{#or @last (if url \"==\" null)}}\r\n                            <span class=\"breadcrumb-label\"><span itemprop=\"name\">{{name}}</span></span>\r\n                        {{else}}\r\n                            <a href=\"{{url}}\" class=\"breadcrumb-label\" itemprop=\"item\"><span itemprop=\"name\">{{name}}</span></a>\r\n                        {{/or}}\r\n                        <meta itemprop=\"position\" content=\"{{@index}}\" />\r\n                    </li>\r\n                {{/or}}\r\n            {{/each}}\r\n        {{/if}}\r\n        {{/comment}}\r\n    {{/unless}}\r\n</ul>\r\n\r\n<script type=\"application/ld+json\">\r\n{\r\n    \"@context\": \"https://schema.org\",\r\n    \"@type\": \"BreadcrumbList\",\r\n    \"itemListElement\":\r\n    [\r\n        {{#each breadcrumbs}}\r\n        {\r\n            \"@type\": \"ListItem\",\r\n            \"position\": {{add @index 1}},\r\n            \"item\": {\r\n                \"@id\": {{{JSONstringify url}}},\r\n                \"name\": {{{JSONstringify name}}}\r\n            }\r\n        }{{#unless @last}},{{/unless}}\r\n        {{/each}}\r\n    ]\r\n}\r\n</script>\r\n"}
