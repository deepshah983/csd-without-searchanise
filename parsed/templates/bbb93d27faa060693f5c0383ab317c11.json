{"components/common/social-links":"{{#if social_media}}\r\n    <ul class=\"socialLinks socialLinks--alt\">\r\n        {{#each social_media}}\r\n            <li class=\"socialLinks-item \">\r\n                <a class=\"icon icon--{{display_name}} {{display_name}}\"\r\n                   href=\"{{url}}\"\r\n                   target=\"_blank\"\r\n                   rel=\"noopener\"\r\n                   title=\"{{{capitalize display_name}}}\"\r\n                >\r\n                    <span class=\"aria-description--hidden\">{{{capitalize display_name}}}</span>\r\n                    {{#if display_name '===' 'twitter'}}\r\n                    <svg xmlns=\"http://www.w3.org/2000/svg\" enable-background=\"new 0 0 72 72\" viewBox=\"0 0 72 72\" id=\"twitter-x\"><switch><g><path d=\"M42.5,31.2L66,6h-6L39.8,27.6L24,6H4l24.6,33.6L4,66    h6l21.3-22.8L48,66h20L42.5,31.2z M12.9,10h8l38.1,52h-8L12.9,10z\"/></g></switch>\r\n                    </svg>\r\n                    {{else}}\r\n                    <svg>\r\n                        <use href=\"#icon-{{display_name}}\"/>\r\n                    </svg>\r\n                    {{/if}}\r\n                </a>\r\n            </li>\r\n        {{/each}}\r\n    </ul>\r\n{{/if}}\r\n"}
