{"components/common/paginator":"<nav class=\"pagination {{#if links}}pagination-num{{/if}}\" aria-label=\"pagination\">\r\n    {{#if previous}}\r\n    <span class=\"pagination-item pagination-item--previous\">\r\n        <a class=\"pagination-link\"\r\n           href=\"{{previous}}\"\r\n           {{#unless reload}}data-faceted-search-facet{{/unless}}\r\n           aria-label=\"{{lang 'common.previous'}}\"\r\n        >\r\n            <i class=\"icon\" aria-hidden=\"true\">\r\n                {{#comment}}<svg><use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-left\"></use></svg>{{/comment}}\r\n                <svg width=\"14px\" height=\"14px\" fill=\"black\" aria-hidden=\"true\" focusable=\"false\" data-prefix=\"fas\" data-icon=\"arrow-left\" role=\"img\" xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 448 512\">\r\n                    <path fill=\"currentColor\" d=\"M257.5 445.1l-22.2 22.2c-9.4 9.4-24.6 9.4-33.9 0L7 273c-9.4-9.4-9.4-24.6 0-33.9L201.4 44.7c9.4-9.4 24.6-9.4 33.9 0l22.2 22.2c9.5 9.5 9.3 25-.4 34.3L136.6 216H424c13.3 0 24 10.7 24 24v32c0 13.3-10.7 24-24 24H136.6l120.5 114.8c9.8 9.3 10 24.8.4 34.3z\"></path>\r\n                </svg>\r\n            </i>\r\n            <i class=\"icon icon-brands\" aria-hidden=\"true\">\r\n                <svg>\r\n                    <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-left\"></use>\r\n                </svg>\r\n            </i>\r\n            {{#comment}}{{lang 'common.previous'}}{{/comment}}\r\n        </a>\r\n    </span>\r\n    {{/if}}\r\n    <ul class=\"pagination-list {{#if links}}pagination-list-multiple{{/if}}\">\r\n        {{#each links}}\r\n        {{assignVar \"foo\" ../current}}\r\n        {{assignVar \"foo1\" (decrementVar \"foo\")}}\r\n        {{assignVar \"foo\" ../current}}\r\n        {{assignVar \"foo2\" (incrementVar \"foo\")}}\r\n        <li class=\"pagination-item {{#if this.number '==' ../current}}pagination-item--current{{/if}}\">\r\n            <a class=\"pagination-link\"\r\n            href=\"{{this.url}}\"\r\n            {{#unless reload}}data-faceted-search-facet{{/unless}}\r\n            {{#if this.number '==' ../current}}\r\n            aria-current=\"page\"\r\n            data-pagination-current-page-link\r\n            {{/if}}\r\n            aria-label=\"{{lang 'common.paginator.page_of' current=this.number total=../links.length}}\"\r\n            >\r\n                {{this.number}}\r\n            </a>\r\n        </li>\r\n        {{/each}}\r\n    </ul>\r\n    {{#if next}}\r\n        <span class=\"pagination-item pagination-item--next\">\r\n            <a class=\"pagination-link\"\r\n                href=\"{{next}}\"\r\n                {{#unless reload}}data-faceted-search-facet{{/unless}}\r\n                aria-label=\"{{lang 'common.next'}}\"\r\n            >\r\n                {{#comment}}{{lang 'common.next'}}{{/comment}}\r\n                <i class=\"icon\" aria-hidden=\"true\">\r\n                    {{#comment}}<svg><use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-right\"></use></svg>{{/comment}}\r\n                    <svg width=\"14px\" height=\"14px\" fill=\"black\" aria-hidden=\"true\" focusable=\"false\" data-prefix=\"fas\" data-icon=\"arrow-left\" role=\"img\" xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 448 512\">\r\n                        <path fill=\"currentColor\" d=\"M190.5 66.9l22.2-22.2c9.4-9.4 24.6-9.4 33.9 0L441 239c9.4 9.4 9.4 24.6 0 33.9L246.6 467.3c-9.4 9.4-24.6 9.4-33.9 0l-22.2-22.2c-9.5-9.5-9.3-25 .4-34.3L311.4 296H24c-13.3 0-24-10.7-24-24v-32c0-13.3 10.7-24 24-24h287.4L190.9 101.2c-9.8-9.3-10-24.8-.4-34.3z\"></path>\r\n                    </svg>\r\n                </i>\r\n                <i class=\"icon icon-brands\" aria-hidden=\"true\" style=\"display: none;\">\r\n                    <svg>\r\n                        <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-right\"></use>\r\n                    </svg>\r\n                </i>\r\n            </a>\r\n        </span>\r\n    {{/if}}\r\n</nav>\r\n","components/search/content-listing":"{{#if content_results}}\r\n    {{> components/search/content-sort-box sort=pagination.content_results.sort}}\r\n    <ul>\r\n        {{#each content_results}}\r\n            <strong><a href=\"{{url}}\">{{{title}}}</a></strong>({{type}})\r\n            <p>{{{content}}}</p>\r\n        {{/each}}\r\n    </ul>\r\n    {{> components/common/paginator pagination.content_results reload=true}}\r\n{{/if}}\r\n","components/search/content-sort-box":"<form class=\"actionBar\" method=\"get\" data-sort-by=\"content\">\r\n    <fieldset class=\"form-fieldset actionBar-section\">\r\n        <div class=\"form-field\">\r\n            <label class=\"form-label\" for=\"sort\">{{lang 'common.sorter.sort_by'}}</label>\r\n            <select class=\"form-select form-select--small\" name=\"sort\" id=\"sort\">\r\n                <option value=\"relevance\" {{#if sort '==' 'relevance'}}selected{{/if}}>{{lang 'common.sorter.relevance'}}</option>\r\n                <option value=\"alphaasc\" {{#if sort '==' 'alphaasc'}}selected{{/if}}>{{lang 'common.sorter.alpha_asc'}}</option>\r\n                <option value=\"alphadesc\" {{#if sort '==' 'alphadesc'}}selected{{/if}}>{{lang 'common.sorter.alpha_desc'}}</option>\r\n            </select>\r\n        </div>\r\n    </fieldset>\r\n</form>\r\n"}
